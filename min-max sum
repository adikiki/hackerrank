void miniMaxSum(vector<int> arr) {
int n=arr.size();
long long int min=0,max=0;

sort(arr.begin(),arr.end());
for (int i=0; i<arr.size()-1; i++) {
             min += arr[i];
         } 
        
        for (int i=arr.size()-1; i>0; i--) {
             max += arr[i];
         }     
  cout<<min<<" "<<max;
  }
